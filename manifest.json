{
  "manifest_version": 2,

  "name": "Blacklist",
  "description": "Allows you to block certain words from all your browser",
  "version": "1.0",
  "options_page": "blackList-options.html",
  "permissions": [
    "storage"
  ],
  "content_scripts": [
  {
    "matches": [
      "<all_urls>"
    ],
    "js": ["jquery-1.11.3.min.js", "content.js"]
  }
]
}
